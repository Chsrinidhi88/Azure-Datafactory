{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryincrementalload"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage22001.dfs.core.windows.net"
		},
		"AzureKeyVaulttoADF_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultsincremental.vault.azure.net/"
		},
		"storageevent_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7aa64674-fa44-4861-867b-78c40d8e8a14/resourceGroups/resource22001/providers/Microsoft.Storage/storageAccounts/storage22001"
		},
		"triggerincrement_properties_pipeline_parameters_windowStartTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs. windowStartTime"
		},
		"triggerincrement_properties_pipeline_parameters_windowEndTime": {
			"type": "string",
			"defaultValue": "@trigger().outputs. windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryincrementalload_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureKeyVaulttoADF_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaulttoADF_properties_typeProperties_baseUrl')]"
					},
					"storageevent_properties_typeProperties_scope": {
						"value": "[parameters('storageevent_properties_typeProperties_scope')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactoryincrementalload_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"triggerincrement_properties_pipeline_parameters_windowStartTime": {
						"value": "[parameters('triggerincrement_properties_pipeline_parameters_windowStartTime')]"
					},
					"triggerincrement_properties_pipeline_parameters_windowEndTime": {
						"value": "[parameters('triggerincrement_properties_pipeline_parameters_windowEndTime')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryincrementalload_ArmTemplate_0')]"
			]
		}
	]
}